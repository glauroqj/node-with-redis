{
  "name": "with-redis",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "----- DOCKER HELPERS -----": "",
    "delete-all": "yarn delete-containers && yarn delete-images && yarn delete-volumes",
    "delete-containers": "docker container stop $(docker container ls -qa) && docker container rm $(docker container ls -qa)",
    "delete-images": "docker image rm $(docker image ls -q)",
    "delete-volumes": "docker volume rm $(docker volume ls -q)",
    "----- DOCKER DEVELOPER -----": "",
    "dev-start": "docker-compose up -d --build && docker ps && yarn dev-logs",
    "dev-stop": "docker-compose stop && docker ps",
    "dev-restart": "yarn dev-stop && yarn dev-start",
    "dev-logs": "docker logs -f node_with_redis",
    "----- DEVELOPER -----": "",
    "dev": "yarn build-server && npm-run-all --parallel dev:*",
    "build-server": "webpack --config webpack/webpack.server.dev.js",
    "build-client": "webpack --config webpack/webpack.client.dev.js",
    "dev:build-server": "webpack --config webpack/webpack.server.dev.js --watch",
    "dev:build-client": "webpack --config webpack/webpack.client.dev.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/server/server.bundle.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glauroqj/node-with-redis.git"
  },
  "author": "Glauro Q Juliani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/glauroqj/node-with-redis/issues"
  },
  "homepage": "https://github.com/glauroqj/node-with-redis#readme",
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^4.0.0",
    "@apollo/react-ssr": "^4.0.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-rest": "^0.8.0-beta.0",
    "connect-redis": "^5.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.11.0",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redis": "^3.0.2",
    "styled-components": "^5.1.1",
    "styled-reset": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression-webpack-plugin": "^5.0.0",
    "dotenv-webpack": "^2.0.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.1"
  }
}
